        <typeAlias alias="RolePojo"              type="com.grgbanking.framework.domains.manager.role.pojo.RolePojo"/>
        <typeAlias alias="EmployeePojo"              type="com.grgbanking.framework.domains.employee.pojo.EmployeePojo"/>
        <typeAlias alias="EmployeeSignPojo"              type="com.grgbanking.framework.domains.employeeSign.pojo.EmployeeSignPojo"/>
        <typeAlias alias="EmployeeDailyPojo"              type="com.grgbanking.framework.domains.employeeDaily.pojo.EmployeeDailyPojo"/>
        <typeAlias alias="employeeDailyRulePojo"              type="com.grgbanking.framework.domains.employeeDailyRule.pojo.EmployeeDailyRulePojo"/>
        <typeAlias alias="monitoringEmployeePojo"              type="com.grgbanking.framework.domains.report.employee.monitoringEmployee.pojo.MonitoringEmployeePojo"/>
        <typeAlias alias="DataDictionaryPojo"              type="com.grgbanking.framework.domains.manager.data_dictionary.pojo.DataDictionaryPojo"/>
        <typeAlias alias="DataDictionaryListJson"              type="com.grgbanking.framework.domains.manager.data_dictionary.json.DataDictionaryListJson"/>
