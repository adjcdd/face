        <typeAlias alias="DataDictionaryPojo"              type="com.grgbanking.framework.domains.manager.data_dictionary.pojo.DataDictionaryPojo"/>
        <typeAlias alias="DataDictionaryListJson"              type="com.grgbanking.framework.domains.manager.data_dictionary.json.DataDictionaryListJson"/>
